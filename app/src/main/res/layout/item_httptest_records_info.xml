<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:background="@drawable/detail_bg"
              xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:gravity="center"
              android:layout_gravity="center"
              android:id="@+id/test_host"
              android:background="@color/main_app"
              android:padding="10.0dip"
              android:text="主页"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              style="@style/TitleText2Appearance"/>
    <LinearLayout android:orientation="vertical"
                  android:id="@+id/http_normal_layout"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content">
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/test_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/test_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/test_address"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/test_address_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/status_code"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/status_code_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/analysis_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/analysis_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/connect_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/connection_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/request_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/request_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/response_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/response_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/first_package_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/first_package_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/finish_time"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/finish_time_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/sense_level"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/sense_level_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/server_ip"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/server_ip_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/local_ip"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/local_ip_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/download_file_size"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/download_file_size"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/download_file_speed"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/download_speed_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <TextView android:gravity="center|left"
                  android:layout_gravity="center"
                  android:id="@+id/header_info"
                  android:background="@drawable/detail_bg"
                  android:padding="10.0dip"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:visibility="gone"
                  style="@style/TitleText2Appearance"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical"
                  android:id="@+id/http_error_layout"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:visibility="gone">
        <LinearLayout android:layout_width="fill_parent"
                      android:layout_height="wrap_content">
            <TextView android:gravity="center|left"
                      android:layout_gravity="center"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:text="@string/http_error"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
            <TextView android:gravity="center|right"
                      android:layout_gravity="center"
                      android:id="@+id/http_error_text"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_weight="1.0"
                      style="@style/TitleText2Appearance"/>
        </LinearLayout>
        <ImageView android:background="@drawable/dot"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
    </LinearLayout>
</LinearLayout>